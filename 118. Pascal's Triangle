class Solution {
    public List<Integer> genaratelist(int row){
        long ans = 1;
        List<Integer> answer = new ArrayList<>();
        answer.add(1);
        for(int i = 1; i<row;i++){
            ans = ans * (row - i);
            ans = ans /i;
            answer.add((int)ans);
        }
        return answer;

    }
    public List<List<Integer>> generate(int numRows) {
        List<List<Integer>> ans= new ArrayList<>();
        for(int i =1; i<=numRows;i++){
            ans.add(genaratelist(i));
        }
        return ans;
    }
}
